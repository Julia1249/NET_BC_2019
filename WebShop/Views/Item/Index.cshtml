@model CatalogModel  
@{
    ViewData["Title"] = "Items";
}

<h2>Items</h2>
<body>
    <div class="container">
        <div class="row">
            
            <div class="col-md-4">
                @foreach (var category in Model.Categories.Where(i => !i.CategoryId.HasValue))
                {
                <div class="list-group">
                    <a href="#" class="list-group-item active">
                        @category.Title
                    </a>
                    @foreach (var subcategory in Model.Categories.Where(r => r.CategoryId == category.Id))
                    {
                        <a class="list-group-item" asp-controller="Item" asp-action="Index" asp-route-id="@subcategory.Id">
                    <span class="badge">4</span>
                            @subcategory.Title
                        </a>
                    }
                </div>
                }
            </div>

            <div class="col-md-8">
                <ol class="breadcrumb">
                    <li><a href="#">Home</a></li>
                    <li><a href="#">Electronics</a></li>
                    <li class="active">Mobile</li>
                </ol>
                <div class="row">
                    @foreach (var item in Model.Items)
                    {
                        <div class="col-md-4">
                            <div class="thumbnail">
                                <img src="data:image/webp;base64," alt="...">
                                <div class="caption text-center">
                                    <h3>@item.Title</h3>
                                    <p>Price: <span>$@item.Price</span></p>
                                    <p><a href="#" class="btn btn-primary" role="button">Add to cart</a> </p>
                                </div>
                            </div>
                        </div>
                    }


                </div>
            </div>
        </div>
    </div>
</body>
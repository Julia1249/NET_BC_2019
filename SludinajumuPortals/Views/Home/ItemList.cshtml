@model CatalogModel
@{
    ViewData["Title"] = "ItemList";
}

<h2>ItemList</h2>

<table class="table table-hover">
    <tr>
        <th colspan="3">Attēls</th>
        <th colspan="7">Apraksts</th>
        <th colspan="2">Cena</th>
    </tr>
    @foreach (var item in Model.Items)
    {
        <tr>
            <td><img src="@item.Photo"></td>
            <td>@item.Description</td>
            <td>$@item.Price</td>
        </tr>
    }

</table>
                